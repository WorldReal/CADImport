{
  "name": "CADImport",
  "version": "0.0.0",
  "scripts": {
    "activate_env": "source activate & conda activate pc_py",
    "dev": "conda activate pc_py & python src/main.py",
    "build": "conda activate pc_py & pyinstaller --distpath=dist --workpath=build  server_main.spec",
    "build_base": "conda activate pc_py & pyinstaller --contents-directory . --icon=source/img/ion/favicon.ico --distpath=dist --workpath=build --add-data=source;source -D  src/server_main.py",
    "list_lib": "conda activate pc_py & conda env export > environment.yaml"
  }
}